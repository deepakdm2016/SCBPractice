def binSearch(li,search):
    f=0
    l=len(li)-1
    
    while(f<=l):
        if(li[int((f+l)/2)]==search):
            print("first If",li[int((f+l)/2)])
            return ("element found")
        
    
        elif(li[int((f+l)/2)]<search):
            print("second If",li[int((f+l)/2)])
            f=li[(f+l)/2]
    
        else:
            print("in else",li[int((f+l)/2)])
            l=li[int((f+l)/2)]
    
    return("Element not found")
            

li=[1,4,6,8,9]
print(binSearch(li, 10))
    
    