def binSearch(li,search):
    f=0
    l=len(li)-1
    
    while(f<=l):
        if(li[int((f+l)/2)]==search):
            print(li[int((f+l)/2)])
            return ("element found")
        
    
        elif(li[int((f+l)/2)]<search):
            print(li[int((f+l)/2)])
            f=li[(f+l)/2]
    
        else:
            l=li[int((f+l)/2)]
            

li=[1,4,6,8,9]
binSearch(li, 6)
    
    